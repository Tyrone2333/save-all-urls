{
  "manifest_version": 3,
  "name": "save-all-urls",
  "description": "This extension save batch of tabs as workspace.",
  "version": "1.1",
  "action": {
    "default_icon": "tabs.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
